{
  "name": "coffee-vector-search-guest",
  "version": "0.1.0",
  "keywords": [],
  "author": "",
  "main": "dist/index.js",
  "scripts": {
    "build:wasm": "jco componentize dist/index.mjs -w ../../../../guests/typescript/wit -o dist/component.wasm",
    "build:wasm:aot": "jco componentize  dist/index.mjs -w ../../../../guests/typescript/wit --aot -o dist/component.wasm",
    "build": "vite build && npm run build:wasm",
    "check": "tsc --noEmit --skipLibCheck && vite build && eslint",
    "deploy": "npm run build && cp dist/component.wasm ../../traildepot/wasm/coffee_vector_search_guest.wasm",
    "format": "prettier -w src"
  },
  "dependencies": {
    "trailbase-wasm": "workspace:*"
  },
  "devDependencies": {
    "@bytecodealliance/jco": "^1.15.4",
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.9",
    "eslint": "^9.39.2",
    "prettier": "^3.8.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vite": "^7.3.1",
    "vitest": "^3.2.4"
  }
}
